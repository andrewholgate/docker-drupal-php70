drupalphp70web:
  build: .
  ports:
    - "81:80"
    - "444:443"
  links:
   - drupalphp70db:database
drupalphp70db:
  image: mysql:5.7
  volumes_from:
    - mysql-drupal-php70
  environment:
    TERM: xterm
    MYSQL_ROOT_PASSWORD: password
    MYSQL_USER: user
    MYSQL_PASSWORD: password
    MYSQL_DATABASE: drupalphp70
